<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--
    This XML file defines the color resources used throughout the application.
    The colors are categorized into primary, secondary, accent, and utility colors.

    Color definitions:
    1. primary_ColorW: The primary white color used in the application.
    2. secondaryColorLG, MG, DG: Variations of green used for secondary branding colors (Light, Medium, Dark).
    3. accendColorB: A bright blue accent color.
    4. dividerColor: A light gray color used for dividers.
    5. gray_background: The background gray color for the UI.
    6. black: Standard black color.
    7. yellow: Used for warnings or moderate alerts.
    8. red: Used for errors or high alerts.

    @author Shawn(Hexuan)
-->

    <color name="primary_ColorW">#FFFFFF</color>


    <color name="secondaryColorLG">#4FB236</color>
    <color name="secondaryColorMG">#24923A</color>
    <color name="secondaryColorDG">#186D37</color>

    <color name="accendColorB">#71C9EE</color>

    <color name="dividerColor">#CCCCCC</color>

    <color name="gray_background">#CCCCCC</color>

    <color name="black">#000000</color>

    <color name="yellow">#FFA500</color>

    <color name="red">#FF0000</color>











</resources>
